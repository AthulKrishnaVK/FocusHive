{
    "manifest_version": 3,
    "name": "FocusHive",
    "version": "1.0",
    "description": "Focus",
    "permissions": ["storage", "scripting", "activeTab","tabs"],
    "host_permissions": ["https://www.youtube.com/*"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/watch*"],
        "js": [
        
        "content.js"
      ],
        "css": ["styles.css"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
      
    }
  }
  
